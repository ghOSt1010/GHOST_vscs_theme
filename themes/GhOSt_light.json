      },
      {
         "scope": ["strong"],
         "settings": {
            "fontStyle": "bold"
         }
      },
      {
         "scope": ["header"],
         "settings": {
            "foreground": "#4f3ccb"
         }
      },
      {
         "scope": ["source"],
